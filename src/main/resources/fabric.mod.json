{
  "schemaVersion": 1,
  "id": "modid",
  "version": "${version}",

  "name": "Armor Extended (Fabric)",
  "description": "This mods changes the armor calculating formula to allow higher armor values to be viable!",
  "authors": [
    "Me!"
  ],
  "contact": {
    "sources": "https://github.com/dev-lukas/armor-extended-fabric"
  },

  "license": "CC0-1.0",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.fabricmc.armorExtended.ArmorExtended"
    ]
  },
  "mixins": [
    "modid.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric-api": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
